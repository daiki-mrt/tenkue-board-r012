<%- if controller_name != 'sessions' %>
  <%= link_to "アカウントをお持ちの方はこちら", new_session_path(resource_name), class: "btn" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録はこちら", new_registration_path(resource_name), class: "btn" %><br />
<% end %>